{
    "name": "env",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/core/env/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/core/env",
                "main": "libs/core/env/src/index.ts",
                "tsConfig": "libs/core/env/tsconfig.lib.json",
                "assets": ["libs/core/env/*.md"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/core/env/jest.config.ts"
            }
        }
    }
}
